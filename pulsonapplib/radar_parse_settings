# The path delay through the antennas (ns)
dT_0=$dT_0$

# Set the one way range distance to start the scan (m)
range_start=$range_start$

# Set the one way range distance to end the scan (m)
range_stop=$range_stop$

# Set the transmit gain from 0 (lowest) - 63 (highest)
tx_gain_ind=$tx_gain_ind$

# Set the pulse integration index from 6 (lowest) - 15 (highest)
pii=$pii$

# Code channel from 0 - 10
code_channel=$code_channel$